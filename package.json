    "files": [
        "packages"
    ],
    "dependencies": {
        "effector": "^21.7.5",
        "effector-react": "^21.2.1",
        "styled-components": "^5.2.1",
        "typescript": "^4.1.2",
        "@fortawesome/fontawesome-svg-core": "^1.2.32",
        "@fortawesome/free-brands-svg-icons": "^5.15.1",
        "@fortawesome/free-regular-svg-icons": "^5.15.1",
        "@fortawesome/free-solid-svg-icons": "^5.15.1",
        "@fortawesome/react-fontawesome": "^0.1.14",
        "@types/node": "^12.19.7",
        "@types/react": "^16.14.2",
        "@types/react-dom": "^16.9.10",
        "react-transition-group": "^4.4.1",
        "@types/react-transition-group": "^4.4.0",
        "@types/styled-components": "^5.1.4"
    },
    "devDependencies": {
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "@babel/cli": "^7.12.8",
        "@babel/core": "^7.9.0",
        "@babel/plugin-transform-runtime": "^7.12.10",
        "@babel/preset-env": "^7.12.7",
        "@babel/preset-react": "^7.12.7",
        "@babel/preset-typescript": "^7.12.7",
        "gh-pages": "^3.1.0",
        "@storybook/addon-actions": "^6.1.11",
        "@storybook/addon-docs": "^6.1.11",
        "@storybook/addon-essentials": "^6.1.11",
        "@storybook/addon-jest": "^6.1.11",
        "@storybook/addon-knobs": "^6.1.11",
        "@storybook/addon-links": "^6.1.11",
        "@storybook/addon-viewport": "^6.1.11",
        "@storybook/addons": "^6.1.11",
        "@storybook/linter-config": "^2.5.0",
        "@storybook/node-logger": "^6.1.11",
        "@storybook/preset-create-react-app": "^3.1.5",
        "@storybook/react": "6.1.11",
        "@storybook/storybook-deployer": "^2.8.7",
        "@svgr/cli": "^5.5.0",
        "@testing-library/jest-dom": "^5.11.6",
        "@testing-library/react": "^11.2.2",
        "@testing-library/user-event": "^12.2.2",
        "@types/jest": "^26.0.15",
        "@types/testing-library__jest-dom": "^5.9.5",
        "@types/testing-library__react": "^10.2.0",
        "@typescript-eslint/eslint-plugin": "^4.8.2",
        "@typescript-eslint/parser": "^4.8.2",
        "babel-loader": "^8.1.0",
        "babel-plugin-inline-react-svg": "1.1.1",
        "babel-plugin-polished": "^1.1.0",
        "babel-plugin-styled-components": "^1.10.7",
        "check-types": "^11.1.2",
        "copyfiles": "^2.4.1",
        "eslint": "^7.2.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-config-airbnb-typescript": "^12.0.0",
        "eslint-config-prettier": "^6.15.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jest": "^24.1.3",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-react": "^7.21.5",
        "eslint-plugin-react-hooks": "^4.0.0",
        "husky": "^4.3.0",
        "jest": "^26.6.3",
        "lint-staged": "^10.5.2",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.2.1",
        "react-scripts": "4.0.1",
        "storybook-addon-react-docgen": "^1.2.42",
        "ts-jest": "^26.4.4",
        "ts-loader": "^8.0.13",
        "web-vitals": "^0.2.4"
    },
    "scripts": {
        "compile-icons": "npx @svgr/cli --out-dir src/icons src/icons --typescript",
        "test:jest": "jest --config config/jest/jest.config.js",
        "test": "jest -c config/jest/jest.config.js",
        "test:watch": "jest -c config/jest/jest.config.js --watch",
        "eject": "react-scripts eject",
        "clear": "rm -rf packages",
        "mkdir": "mkdir -m 700  packages",
        "copy": "cp -r src/theme/fonts packages/theme/",
        "build": "tsc && npm run copy",
        "storybook": "start-storybook -p 6006 -s public",
        "build-storybook": "build-storybook -s public",
        "prebuild": "npm run clear && npm run compile-icons && npm run mkdir",
        "lint": "tsc --noEmit && eslint src/**/*.ts{,x}",
        "lint:fix": "eslint --ext js,js,ts,tsx src --fix",
        "format": "prettier --write src/**/*.ts{,x}",
        "deploy": "gh-pages -d storybook-static",
        "predeploy": "npm run build-storybook",
        "deploy-storybook": "gh-pages -d storybook-static"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npx lint-staged"
        }
    },
    "lint-staged": {
        "*.+(js|jsx|ts|tsx)": [
            "eslint --fix",
            "git add"
        ],
        "*.+(json|css|md)": [
            "prettier --write",
            "git add"
        ]
    },
    "jest": {
        "setupFilesAfterEnv": [
            "@testing-library/jest-dom/extend-expect",
            "<rootDir>/src/setupTests.js"
        ]
    },
    "publishConfig": {
        "registry": "https://npm.pkg.github.com"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "description": "This project was bootstrapped with [Create React App](https://github.com/facebook/create-react-app).",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/scalereal/scalereal-components.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/scalereal/scalereal-components/issues"
    }
}
